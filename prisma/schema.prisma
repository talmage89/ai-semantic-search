generator client {
  provider = "prisma-client-js"
  previewFeatures = ["omitApi"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id                   String  @id @default(cuid())
  email                String  @unique
  password             String
  emailValidationToken String?
  emailVerified        Boolean @default(false)
  name                 String?
}
